{
  "name": "solana-liquidity-bot",
  "version": "1.0.0",
  "description": "Telegram bot that tracks Solana contract addresses and alerts when liquidity is added",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "node -r ts-node/register src/index.ts",
    "dev-js": "node test-simple.js",
    "watch": "tsc --watch",
    "test-bot": "node scripts/test-bot.js",
    "test-simple": "node test-simple.js",
    "setup": "echo 'Please follow the setup guide in scripts/setup.md'"
  },
  "keywords": [
    "telegram",
    "bot",
    "solana",
    "liquidity",
    "defi"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "telegraf": "^4.15.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
